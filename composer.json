{
    "name": "subvert/subvert",
    "authors": [
        {
            "name": "chenchun",
            "email": "chenchun0629@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path", 
            "url": "packages/subvert"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "subvert/subvert-framework": "*@dev",
        "ramsey/uuid": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Base\\" : "app/Base/",
            "Bll\\"  : "app/Bll/",
            "Com\\"  : "app/Com/",
            "Logic\\": "app/Logic/",
            "Sass\\" : "app/Sass/",
            "Store\\": "store/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    }
}
